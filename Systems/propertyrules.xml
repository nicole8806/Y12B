<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>

  
  <filter>
			<name>right-arm</name>
			<debug>true</debug>
			<type>gain</type>
			<gain>1.0</gain>
			<input>
				<property>controls/doors/door3/position-norm</property>
				<scale>160.0</scale>
				<offset>-80.0</offset>
			</input>
			<!-- arm down: -85 up: +80 -->
			<output>
				<property>controls/doors/door3/position-out</property>
			</output>
	</filter>
  
  

</PropertyList>
